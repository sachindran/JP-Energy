<?xml version='1.0' encoding='utf-8'?>
<widget id="com.thekbsystems.jpenergy" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>JP-Energy</name>
    <description>
        Mobile app of JP Energy Optimization to monitor real time data of various paramteres in Jefferson Parish County.
    </description>
    <author email="thekbsystems@gmail.com" href="http://www.thekbsystems.com/JPEOpti/Admin/DataDisplay.aspx">
        The KB Systems
    </author>
    <content src="index.html" />
    <access origin="*" />

    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarOverlaysWebView" value="true"/>
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="lightcontent" />
</widget>
